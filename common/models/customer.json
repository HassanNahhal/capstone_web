{
  "name": "Customer",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },    
    "dateOfBirth": {
      "type": "date"
    },
    "gender": {
      "type": "number"
    },
    "groupId": {
      "type": "string",
      "id": false,
      "required": false,
      "index": false
    }
  },
  "validations": [],
  "relations": {
    "receipts": {
      "type": "hasMany",
      "model": "Receipt",
      "foreignKey": ""
    },
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "accountId"
    },
    "groups": {
      "type": "hasMany",
      "model": "Group",
      "foreignKey": "",
      "through": "CustomerGroup"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
